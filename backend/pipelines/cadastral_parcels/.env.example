# API Credentials for Datafordeleren
DATAFORDELER_USERNAME=your_username
DATAFORDELER_PASSWORD=your_password
DATAFORDELER_URL=https://services.datafordeler.dk/Matrikel/MatrikelGeometri/1.0.0/WFS

# Environment (dev or prod)
ENVIRONMENT=dev

# Google Cloud Storage (only used in prod environment)
GCS_BUCKET_NAME=your-bucket-name

# Pipeline Configuration
# Number of features per WFS request
CADASTRAL_PAGE_SIZE=1000
# Number of features per batch write
CADASTRAL_BATCH_SIZE=5000
# Maximum number of concurrent requests
CADASTRAL_MAX_CONCURRENT=5
# Request timeout in seconds
CADASTRAL_REQUEST_TIMEOUT=300
# Total pipeline timeout in seconds
CADASTRAL_TOTAL_TIMEOUT=7200
# Maximum requests per second (rate limiting)
CADASTRAL_REQUESTS_PER_SECOND=2